@page
@model TransporteBusesApp.Frontend.Pages.DetailsEstacionModel
@{
    var estacion = Model.Estacion;
}

<div class="page-heading">
    <div class="container">
        <div class="row">
            <div class="col-lg-8">
                <div class="top-text header-text">
                <h6>Detalle del Registro</h6>
                 <!--<h2>Registra el Bus</h2>-->
                </div>
            </div>
        </div>
    </div>
</div>


<ul class="list-group list-group-flush">
  <li class="list-group-item"> <b>id: </b> @estacion.id</li>
  <li class="list-group-item"> <b>Nombre: </b> @estacion.nombre</li>
  <li class="list-group-item"> <b>Direccion: </b> @estacion.direccion</li>
  <li class="list-group-item"><b>Coordenada X: </b> @estacion.coord_x</li>
  <li class="list-group-item"><b>Coordenada Y: </b> @estacion.coord_y</li>
  <li class="list-group-item"><b>Tipo: </b> @estacion.tipo</li>
</ul>  